<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iit.androidnewsappspl2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListNews" />
        <activity android:name=".DetailArticle"></activity>
        <activity android:name=".ItemCategoryList"></activity>
        <activity android:name=".YourWishesAboutSources"></activity>

        <activity android:name=".PreferSourcesList"></activity>
        <activity android:name=".BlockSourcesList"></activity>
        <activity android:name=".PreferListNews"></activity>
        <activity android:name=".InterestSearchActivity"></activity>



    </application>

</manifest>